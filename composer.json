{
    "name": "bk2k/packagebuilder",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.4",
        "doctrine/annotations": "^1.14.3",
        "doctrine/cache": "^1.13",
        "jms/serializer-bundle": "^3.10",
        "knplabs/knp-menu-bundle": "^3.2",
        "nelmio/api-doc-bundle": "^3.10.1",
        "sensio/framework-extra-bundle": "^5.6.1",
        "symfony/asset": "^5.4.21",
        "symfony/console": "^5.4.23",
        "symfony/dotenv": "^5.4.22",
        "symfony/expression-language": "^5.4.21",
        "symfony/finder": "^5.4.21",
        "symfony/flex": "^1.19.5",
        "symfony/form": "^5.4.23",
        "symfony/framework-bundle": "^5.4.22",
        "symfony/mime": "^5.4.23",
        "symfony/options-resolver": "^5.4.21",
        "symfony/runtime": "^5.4.22",
        "symfony/security-csrf": "^5.4.21",
        "symfony/serializer": "^5.4.23",
        "symfony/templating": "^5.4.21",
        "symfony/twig-bundle": "^5.4.21",
        "symfony/validator": "^5.4.23",
        "symfony/webpack-encore-bundle": "^1.16.1",
        "symfony/yaml": "^5.4.23"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.14.2",
        "symfony/stopwatch": "^5.4.21",
        "symfony/web-profiler-bundle": "^5.4.21"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "cgl": [
            "php-cs-fixer --diff -v fix"
        ],
        "cgl:ci": [
            "php-cs-fixer --diff -v --dry-run fix"
        ]
    },
    "config": {
        "bin-dir": "bin",
        "vendor-dir": "vendor",
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    }
}
